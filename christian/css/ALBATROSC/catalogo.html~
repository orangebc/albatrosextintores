<script>
$(document).ready(function()
          {
          /*-------------------Abrir------------------------------*/
          $("#boton_menu_open").click(function () {
                          $('#scroll_menu').slideDown('down');
                          $('#boton_menu_close').show();
                          $('#boton_menu_open').hide();
              });
          /*-------------------Cerrar------------------------------*/
          $("#boton_menu_close").click(function () {
                          $('#scroll_menu').slideUp('down');
                          $('#boton_menu_close').hide();
                          $('#boton_menu_open').show();
              });
			  
          /*-------------------Abrir------------------------------*/
          $("#boton_menu_open1").click(function () {
                          $('#scroll_menu1').slideDown('down');
                          $('#boton_menu_close1').show();
                          $('#boton_menu_open1').hide();
              });
          /*-------------------Cerrar------------------------------*/
          $("#boton_menu_close1").click(function () {
                          $('#scroll_menu1').slideUp('down');
                          $('#boton_menu_close1').hide();
                          $('#boton_menu_open1').show();
              });			  
			  
 			  
});
</script>
